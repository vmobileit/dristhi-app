<form autocomplete="off" novalidate="novalidate" class="or clearfix" id="TT_EngKan">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 id="form-title">TT</h3>


    <label class="question non-select ">
        <span class="question-label active" lang="">ANC Number:</span>
        <input name="/TT_EngKan/anc_reg_num" autocomplete="off" data-type-xml="int"
            disabled="disabled" type="text" />
    </label>


    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Which TT injection did you give? </span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input autocomplete="off" type="radio" name="/TT_EngKan/tt_dose" data-name="/TT_EngKan/tt_dose"
                           value="tt1" required="required" data-type-xml="select1"/>
                    <span lang="" class="option-label active">TT1 </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/TT_EngKan/tt_dose" data-name="/TT_EngKan/tt_dose"
                           value="tt2" required="required" data-type-xml="select1"/>
                    <span lang="" class="option-label active">TT2 </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/TT_EngKan/tt_dose" data-name="/TT_EngKan/tt_dose"
                           value="ttbooster" required="required" data-type-xml="select1"/>
                    <span lang="" class="option-label active">TT Booster </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question or-appearance-minimal ">
        <span lang="" class="question-label active">Place of TT injection? </span>
        <span class="required">*</span>
        <select name="/TT_EngKan/tt_injection_place" data-name="/TT_EngKan/tt_injection_place" required="required"
                data-type-xml="select1">
            <option value="">...</option>
            <option value="sub_center">My Sub Center </option>
            <option value="phc">My PHC </option>
            <option value="elsewhere">Elsewhere </option>
        </select>
        <span class="or-option-translations" style="display:none;">
        </span>
    </label>
    <label class="question non-select ">
        <span lang="" class="question-label active">Date of TT injection? </span>
        <span class="required">*</span>
        <input autocomplete="off" type="date" name="/TT_EngKan/tt_date" required="required"
               data-constraint=".&lt;= /TT_EngKan/today " data-type-xml="date"/>
    </label>

    <fieldset id="or-preload-items" style="display:none;">
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/TT_EngKan/today" data-preload="date"
                   data-preload-params="today" data-type-xml="date"/>
        </label>
    </fieldset>
    <fieldset id="or-calculated-items" style="display:none;">
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/TT_EngKan/meta/instanceID"
                   data-calculate="concat('uuid:', uuid())" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/TT_EngKan/formhub/uuid"
                   data-calculate="'33f85fb5d3d4447aa2919f3976a5a4ee'" data-type-xml="string"/>
        </label>
    </fieldset>
</form>
