<form autocomplete="off" novalidate="novalidate" class="or clearfix" id="Vitamin_A_EngKan">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 id="form-title">Vitamin A</h3>


    <label class="question or-appearance-minimal ">
        <span lang="" class="question-label active">Which dose of Vitamin A was given? 
        </span>
        <span class="required">*</span>
        <select name="/Child_Vitamin_A_EngKan/vit_a_dose" data-name="/Child_Vitamin_A_EngKan/vit_a_dose"
                required="required" data-type-xml="select1">
            <option value="">...</option>
            <option value="1">1</option>
            <option value="2">2</option>
            <option value="3">3</option>
            <option value="4">4</option>
            <option value="5">5</option>
            <option value="6">6</option>
            <option value="7">7</option>
            <option value="8">8</option>
            <option value="9">9</option>
        </select>
        <span class="or-option-translations" style="display:none;">
        </span>
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Where was Vitamin A given? </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input autocomplete="off" type="radio" name="/Child_Vitamin_A_EngKan/vit_a_place"
                           data-name="/Child_Vitamin_A_EngKan/vit_a_place" value="sub_center" data-type-xml="select1"/>
                    <span lang="" class="option-label active">My Sub Center </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/Child_Vitamin_A_EngKan/vit_a_place"
                           data-name="/Child_Vitamin_A_EngKan/vit_a_place" value="phc" data-type-xml="select1"/>
                    <span lang="" class="option-label active">My PHC </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/Child_Vitamin_A_EngKan/vit_a_place"
                           data-name="/Child_Vitamin_A_EngKan/vit_a_place" value="elsewhere" data-type-xml="select1"/>
                    <span lang="" class="option-label active">Elsewhere
                        </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question non-select ">
        <span lang="" class="question-label active">Vitamin A date? 
        </span>
        <span class="required">*</span>
        <input autocomplete="off" type="date" name="/Child_Vitamin_A_EngKan/vit_a_date" required="required"
               data-constraint=".&gt;=  /Child_Vitamin_A_EngKan/eight_months  and .&lt;= /Child_Vitamin_A_EngKan/today "
               data-type-xml="date"/>
    </label>

    <fieldset id="or-preload-items" style="display:none;">
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/Child_Vitamin_A_EngKan/today" data-preload="date"
                   data-preload-params="today" data-type-xml="date"/>
        </label>
    </fieldset>
    <fieldset id="or-calculated-items" style="display:none;">
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/Child_Vitamin_A_EngKan/eight_months"
                   data-calculate=" /Child_Vitamin_A_EngKan/case_child_birth_date  + 243" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/Child_Vitamin_A_EngKan/meta/instanceID"
                   data-calculate="concat('uuid:', uuid())" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/Child_Vitamin_A_EngKan/formhub/uuid"
                   data-calculate="'75e97af4cc4843dfae933a478ec901e6'" data-type-xml="string"/>
        </label>
    </fieldset>
</form>
